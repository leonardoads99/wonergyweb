@model vonergyDom.domain.DispositivoIOT

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Content/css")

<div class="row">
    <h3 class="text-center" id="h3">Detalhes do Dispositivo</h3>
    <hr />
    <div class=" form-group col-md-offset-1">
        @Html.ActionLink("Voltar", "ListarDispositivo", "DispositivoIot", new { @class = "btn btn-primary" })
    </div>
</div>

<div class="col-md-4 col-md-offset-4">
    <div class="panel panel-primary ">
        <div class="panel-body">
            <div class="row col-md-offset-2">
                <p>@Html.DisplayNameFor(model => model.Id):@Html.DisplayFor(model => model.Id)</p>
                <p>@Html.DisplayNameFor(model => model.CodigoEsp) : @Html.DisplayFor(model => model.CodigoEsp)</p>
                <p>@Html.DisplayNameFor(model => model.Nome) : @Html.DisplayFor(model => model.Nome)</p>
                <p>@Html.DisplayNameFor(model => model.Marca) : @Html.DisplayFor(model => model.Marca)</p>
                <p>@Html.DisplayNameFor(model => model.Modelo) : @Html.DisplayFor(model => model.Modelo)</p>
                <p>@Html.DisplayNameFor(model => model.Status) : @Html.DisplayFor(model => model.Status)</p>
                <p>@Html.DisplayNameFor(model => model.Descicao) :   @Html.DisplayFor(model => model.Descicao)</p>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/bootstrap")




