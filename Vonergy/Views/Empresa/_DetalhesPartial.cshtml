@model vonergyDom.domain.Empresa

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Content/css")

<div class="row">
    <h3 class="text-center" id="h3">Detalhes da Empresa</h3>
    <hr />
    <div class=" form-group col-md-offset-1">
        @Html.ActionLink("Voltar", "ListarEmpresa", "Empresa", new { @class = "btn btn-primary" })
    </div>
</div>

<div class="col-md-4 col-md-offset-4">
    <div class="panel panel-primary ">
        <div class="panel-body">
            <div class="row col-md-offset-2">
                <p>@Html.DisplayNameFor(model => model.Cnpj) : @Html.DisplayFor(model => model.Cnpj)</p>
                <p>@Html.DisplayNameFor(model => model.RazaoSocial) : @Html.DisplayFor(model => model.RazaoSocial)</p>
                <p>@Html.DisplayNameFor(model => model.NomeFantasia) : @Html.DisplayFor(model => model.NomeFantasia)</p>
                <p>@Html.DisplayNameFor(model => model.Email) : @Html.DisplayFor(model => model.Email)</p>
                <p>@Html.DisplayNameFor(model => model.Telefone) : @Html.DisplayFor(model => model.Telefone)</p>
                <p>@Html.DisplayNameFor(model => model.Celular) : @Html.DisplayFor(model => model.Celular)</p>
                <p>@Html.DisplayNameFor(model => model.Site) : @Html.DisplayFor(model => model.Site)</p>
                <p>@Html.DisplayNameFor(model => model.Logradouro) : @Html.DisplayFor(model => model.Logradouro)</p>
                <p>@Html.DisplayNameFor(model => model.Cidade) : @Html.DisplayFor(model => model.Cidade)</p>
                <p>@Html.DisplayNameFor(model => model.Bairro) : @Html.DisplayFor(model => model.Bairro)</p>
                <p>@Html.DisplayNameFor(model => model.Cep) : @Html.DisplayFor(model => model.Cep)</p>
                <p>@Html.DisplayNameFor(model => model.Uf) : @Html.DisplayFor(model => model.Uf)</p>
                <p>@Html.DisplayNameFor(model => model.Referencia) : @Html.DisplayFor(model => model.Referencia)</p>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/bootstrap")




